{

  "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },

  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "import",
    "react-hooks"
  ],
  "rules": {
    "react/forbid-prop-types": "off",
    "implicit-arrow-linebreak": 0,
    "react/jsx-filename-extension": 0
  }
}